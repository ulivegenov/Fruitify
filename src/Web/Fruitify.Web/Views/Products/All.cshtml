@model Fruitify.Web.ViewModels.Administration.Products.ProductWebAllModel

@{
    this.ViewData["Title"] = "All Products User Page";
}

<section class="ftco-section">
    <div class="container">
        <div class="row">
            @foreach (var product in Model.Products)
            {
                <div class="col-md-6 col-lg-3">
                    <partial name="ProductPartial" model="product" />
                </div>
            }
        </div>
    </div>

    <hr />

    <partial name="PaginationPartial" />
</section>
